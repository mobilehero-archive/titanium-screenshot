				"name": "TiScreenshotDetection",
				"version": "1.0.0",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "8.2.0",
				"guid": "46ce9d7f-fd1d-4618-b4d7-bc6327ea7543"
