		{
			"name": "Brenton House",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:mobilehero-archive/titanium-screenshot"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/mobilehero-archive/titanium-screenshot/issues"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
